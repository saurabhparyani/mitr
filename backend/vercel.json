{
    "version": 2,
    "builds": [
        {
            "src": "backend/",
            "use": "@vercel/python"
        },
        {
            "src": "frontend/",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/wsgi.py"
        }
    ]
}